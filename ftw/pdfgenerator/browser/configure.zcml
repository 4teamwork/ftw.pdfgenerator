<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">

  <browser:page
      for="*"
      name="export_pdf"
      class=".views.ExportPDFView"
      permission="zope2.View"
      />

  <browser:page
      for="*"
      name="debug-pdf"
      class=".views.DebugPDFGeneratorView"
      permission="zope2.View"
      />

</configure>
